<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_home"
    app:startDestination="@id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.androidpractice.home.HomeFragment"
        tools:layout="@layout/fragment_home"/>

    <fragment
        android:id="@+id/coordinateMainFragment"
        android:name="com.example.androidpractice.coordinate.CoordinateMainFragment"
        android:label="fragment_coordinate_main"
        tools:layout="@layout/fragment_coordinate_main"/>
    <action
        android:id="@+id/action_to_coordinateMainFragment"
        app:destination="@id/coordinateMainFragment"/>

    <fragment
        android:id="@+id/webFragment"
        android:name="com.example.androidpractice.web.WebFragment"
        android:label="fragment_web"
        tools:layout="@layout/fragment_web"/>
    <action
        android:id="@+id/action_to_webFragment"
        app:destination="@id/webFragment"/>

    <fragment
        android:id="@+id/luckFragment"
        android:name="com.example.androidpractice.luck.LuckFragment"
        android:label="fragment_luck"
        tools:layout="@layout/fragment_luck"/>
    <action
        android:id="@+id/action_to_luckFragment"
        app:destination="@id/luckFragment"/>

    <fragment
        android:id="@+id/txssFragment"
        android:name="com.example.androidpractice.txss.TxssFragment"
        android:label="fragment_txss"
        tools:layout="@layout/fragment_txss"/>
    <action
        android:id="@+id/action_to_txssFragment"
        app:destination="@id/txssFragment"/>

</navigation>